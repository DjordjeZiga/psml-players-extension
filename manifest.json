{
  "name": "Manager league players",
  "version": "1.4.0",
  "description": "Extension for manager leagues players",
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://pesdb.net/*"],
      "js": ["scripts/main.js"]
    }
  ],
  "manifest_version": 3
}
